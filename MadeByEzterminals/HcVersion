local state = false
local greenMarker = nil

function makemarker(Parent, Adornee, Color, Size, Size2)
    local e = Instance.new("BillboardGui", Parent)
    e.Name = "GreenMarker"
    e.Adornee = Adornee
    e.Size = UDim2.new(Size, Size2, Size, Size2)
    e.AlwaysOnTop = true
    local a = Instance.new("Frame", e)
    a.Size = UDim2.new(1, 0, 1, 0)
    a.BackgroundColor3 = Color
    local g = Instance.new("UICorner", a)
    g.CornerRadius = UDim.new(1, 1)
    return e
end

function updateMarkerPosition()
    if greenMarker and AimPart then
        greenMarker.Adornee = AimPart
    end
end

function toggleBate()
    state = not state
    if state then
        TextButton.Text = "Bate ON"
        enabled = true
        Plr, AimPart = FindNearestEnemy()
        if AimPart then
            if greenMarker then
                greenMarker:Destroy()
            end
            greenMarker = makemarker(game.Workspace, AimPart, Color3.new(0, 1, 0), 0.3, 3)
            updateMarkerPosition()
        end
    else
        TextButton.Text = "Toggle Bate Aimlock"
        enabled = false
        if greenMarker then
            greenMarker:Destroy()
            greenMarker = nil
        end
    end
end

TextButton.MouseButton1Click:Connect(toggleBate)

StrafeButton.MouseButton1Click:Connect(function()
    targetstrafe_enabled = not targetstrafe_enabled
    if targetstrafe_enabled then
        StrafeButton.Text = "Disable targetstrafe"
    else
        StrafeButton.Text = "Enable targetstrafe"
    end
end)

local function isAboveGround(position)
    local ray = Ray.new(position, Vector3.new(0, 0.06, 0) * 5)
    local part, hitPosition = game.Workspace:FindPartOnRay(ray, game.Workspace, false, true)
    return not part
end

function getTargetPosition()
    if AimPart then
        local pos = AimPart.Position
        return { x = pos.X, y = pos.Y, z = pos.Z }
    end
    return nil
end

function setCharacterOrientation(x, y, z)
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character then
        local hrp = character:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = CFrame.new(x, y, z)
        end
    end
end

function getCurrentTime()
    return os.clock()
end

function PreventGroundHits()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character then
        local hrp = character:FindFirstChild("HumanoidRootPart")
        if hrp then
            local currentPosition = hrp.Position
            
            local ray = Ray.new(currentPosition, Vector3.new(0, 0.06, 0) * 50)
            local part, hitPosition = game.Workspace:FindPartOnRay(ray, game.Workspace, false, true)
            
            if part and hitPosition.Y < currentPosition.Y then
                local newPosition = Vector3.new(currentPosition.X, hitPosition.Y + 5, currentPosition.Z)
                hrp.CFrame = CFrame.new(newPosition)
            end
        end
    end
end

function update()
    if enabled and state and greenMarker and AimPart and targetstrafe_enabled then
        local currentTime = getCurrentTime()
        local angle = currentTime * getgenv().targetstrafe.speed
        
        local targetPos = getTargetPosition()
        if targetPos then
            local offsetX = getgenv().targetstrafe.size * math.cos(angle)
            local offsetZ = getgenv().targetstrafe.size * math.sin(angle)
            local offsetY = getgenv().targetstrafe.height  -- Apply vertical offset
            
            local newPos = Vector3.new(targetPos.x + offsetX, targetPos.y + offsetY, targetPos.z + offsetZ)
            
            if NoGroundShots and not isAboveGround(newPos) then
                local ray = Ray.new(newPos, Vector3.new(0, 0.06, 0) * 100)
                local part, hitPosition = game.Workspace:FindPartOnRay(ray, game.Workspace, false, true)
                if part then
                    newPos = hitPosition + Vector3.new(0, 2, 0)
                end
            end

            PreventGroundHits()
            setCharacterOrientation(newPos.x, newPos.y, newPos.z)
        end
    end
end

RunService.RenderStepped:Connect(update)

RunService.Stepped:Connect(function()
    if enabled then
        PredictionValue = generatePredictionValue()
    end
end)

local function generatePredictionValue()
    return PredictionValue
end

local function updatePredictionValue()
    if AutoPred then
        local ping = tonumber(Stats.Network.ServerStatsItem["Data Ping"]:GetValueString():split(".")[1])
        if ping < 20 then
            PredictionValue = 0.11378905
        elseif ping < 30 then
            PredictionValue = 0.1222789
        elseif ping < 40 then
            PredictionValue = 0.12948805
        elseif ping < 50 then
            PredictionValue = 0.137988957
        elseif ping < 60 then
            PredictionValue = 0.1454058899
        elseif ping < 70 then
            PredictionValue = 0.1542885922
        elseif ping < 80 then
            PredictionValue = 0.158893
        elseif ping < 90 then
            PredictionValue = 0.16228392
        elseif ping < 100 then
            PredictionValue = 0.168799
        elseif ping < 110 then
            PredictionValue = 0.172298805
        elseif ping < 120 then
            PredictionValue = 0.17538895
        elseif ping < 130 then
            PredictionValue = 0.181183905
        elseif ping < 140 then
            PredictionValue = 0.1868334505
        elseif ping < 150 then
            PredictionValue = 0.1888428905
        elseif ping < 160 then
            PredictionValue = 0.189328482
        elseif ping < 170 then
            PredictionValue = 0.192283831
        elseif ping < 180 then
            PredictionValue = 0.198837284
        elseif ping < 190 then
            PredictionValue = 0.20348289
        elseif ping < 200 then
            PredictionValue = 0.209739292
        end
    end
end

local mt = getrawmetatable(game)
local old = mt.__index
setreadonly(mt, false)

local lastTarget = nil

mt.__index = newcclosure(function(self, key)
    if not checkcaller() and enabled and typeof(self) == "Instance" and self:IsA("Mouse") and key == "Hit" then
        if Plr and Plr.Character and Plr.Character:FindFirstChild(SelectedPart) and Plr.Character:FindFirstChild("Head") then
            local target = Plr.Character[SelectedPart]
            if target ~= lastTarget then
                lastTarget = target
            end

            local predictedPosition = target.Position + (Plr.Character.Head.Velocity * PredictionValue)
            return CFrame.new(predictedPosition)
        end
    end
    return old(self, key)
end)

setreadonly(mt, true)

local delayTime = 0.182489-- Reduc response
local lastShotTime = 0.182489-- Reduced 0.186394
local autoShootEnabled = false

function autoShoot()
    if autoShootEnabled then
        local tool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
        if tool and Plr then
            local humanoid = Plr.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                local state = humanoid:GetState()
                if state == Enum.HumanoidStateType.Freefall then -- Freefall might be better for detecting air shots
                    local currentTime = tick()
                    if currentTime - lastShotTime >= delayTime then
                        tool:Activate()
                        lastShotTime = currentTime
                    end
                end
            end
        end
    end
end

