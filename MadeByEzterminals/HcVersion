local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function RemoveAppearanceForPlayer(player)
    if player.UserId ~= LocalPlayer.UserId then
        local Character = player.Character or player.CharacterAdded:Wait()
        if Character then
            for _, accessory in pairs(Character:GetChildren()) do
                if accessory:IsA("Accessory") or accessory:IsA("Hair") then
                    accessory:Destroy()
                end
            end
        end
    end
end

local function RemoveAppearanceForAll()
    for _, player in ipairs(Players:GetPlayers()) do
        RemoveAppearanceForPlayer(player)
    end
end

local function ContinuouslyRemoveAppearance()
    game:GetService("RunService").Heartbeat:Connect(function()
        for _, player in ipairs(Players:GetPlayers()) do
            RemoveAppearanceForPlayer(player)
        end
    end)
end

RemoveAppearanceForAll()

Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function()
        RemoveAppearanceForPlayer(player)
    end)
end)

LocalPlayer.CharacterAdded:Connect(function()
    RemoveAppearanceForAll()
end)

ContinuouslyRemoveAppearance()
