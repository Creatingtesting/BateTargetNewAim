local function applySettings()
    -- Apply prediction value if AutoPred is off
    if not getgenv().AutoPred then
        local predictionValue = tonumber(predictionBox.Text)
        if predictionValue then
            PredictionValue = predictionValue
        end
    end

    -- Apply strafe settings
    local heightValue = tonumber(heightBox.Text)
    if heightValue then
        getgenv().targetstrafe.height = heightValue
    end

    local speedValue = tonumber(speedBox.Text)
    if speedValue then
        getgenv().targetstrafe.speed = speedValue
    end

    local sizeValue = tonumber(sizeBox.Text)
    if sizeValue then
        getgenv().targetstrafe.size = sizeValue
    end
end

predictionBox.FocusLost:Connect(applySettings)
heightBox.FocusLost:Connect(applySettings)
speedBox.FocusLost:Connect(applySettings)
sizeBox.FocusLost:Connect(applySettings)

local function toggleTabVisibility()
    if predictionTab.BackgroundColor3 == Color3.new(0.6, 0.6, 0.6) then
        predictionTab.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
        strafeTab.BackgroundColor3 = Color3.new(0.6, 0.6, 0.6)
        predictionBox.Visible = false
        autoPredictionToggle.Visible = false
        strafeSettingsFrame.Visible = true
    else
        predictionTab.BackgroundColor3 = Color3.new(0.6, 0.6, 0.6)
        strafeTab.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
        predictionBox.Visible = true
        autoPredictionToggle.Visible = true
        strafeSettingsFrame.Visible = false
    end
end

predictionTab.MouseButton1Click:Connect(toggleTabVisibility)
strafeTab.MouseButton1Click:Connect(toggleTabVisibility)

