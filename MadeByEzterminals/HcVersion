loadstring(game:HttpGet("https://raw.githubusercontent.com/Pixeluted/adoniscries/main/Source.lua",true))()
getgenv().targetstrafe = {
    speed = 15,
    height = 6,
    size = 6,
}

local aimParts = {"HumanoidRootPart", "Head", "RightLeg", "LeftLeg", "RightArm", "LeftArm", "UpperTorso"}
local SelectedPart = "Head"
local Prediction = true
local PredictionValue = 0.117392
local NoGroundShots = true
local AutoPred = true
Vector3.new(0, 0.1334, 0)

local function groundShots()
Prediction = math.clamp(Velocity.Y * PredictionValue, -0.855, math.huge)
end
function FindNearestEnemy()
    local LocalPlayer = game.Players.LocalPlayer
    local ClosestDistance, ClosestPlayer, ClosestPart = math.huge, nil, nil

    for _, Player in ipairs(game.Players:GetPlayers()) do
        if Player ~= LocalPlayer then
            local Character = Player.Character
            if Character and Character:FindFirstChild("Humanoid") and Character.Humanoid.Health > 0 then
                local Part = Character:FindFirstChild(SelectedPart)
                if Part then
                    local Position, IsVisibleOnViewPort = game:GetService("Workspace").CurrentCamera:WorldToViewportPoint(Part.Position)
                    if IsVisibleOnViewPort then
                        local Distance = (LocalPlayer.Character.Head.Position - Part.Position).Magnitude
                        if Distance < ClosestDistance then
                            ClosestPlayer = Player
                            ClosestPart = Part
                            ClosestDistance = Distance
                        end
                    end
                end
            end
        end
    end

    return ClosestPlayer, ClosestPart
end

local CC = game:GetService("Workspace").CurrentCamera
local Plr, AimPart
local enabled = true
local targetstrafe_enabled = true

local RunService = game:GetService("RunService")

local gui = Instance.new("ScreenGui")
gui.Name = "patrickGui"
